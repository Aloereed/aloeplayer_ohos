{
  module: {
    name: "entry",
    type: "entry",
    description: "$string:module_desc",
    mainElement: "EntryAbility",
    deviceTypes: [
      "phone",
      "tablet"
    ],
    deliveryWithInstall: true,
    installationFree: false,
    pages: "$profile:main_pages",
    abilities: [
      {
        name: "EntryAbility",
        srcEntry: "./ets/entryability/EntryAbility.ets",
        description: "$string:EntryAbility_desc",
        icon: "$media:layered_image",
        orientation: "auto_rotation_restricted",
        label: "$string:EntryAbility_label",
        startWindowIcon: "$media:app_startwindow",
        startWindowBackground: "$color:start_window_background",
        exported: true,
        removeMissionAfterTerminate: true,
        backgroundModes: [
          "audioPlayback",
          "taskKeeping",
          "dataTransfer"
        ],
        skills: [
          {
            entities: [
              "entity.system.home"
            ],
            actions: [
              "action.system.home",
              "ohos.want.action.sendData",
              "ohos.want.action.viewData"
            ],
            uris: [
              {
                scheme: "file",
                type: "general.audio",
                "linkFeature": "FileOpen",
              },
              {
                scheme: "file",
                type: "general.video",
                "linkFeature": "FileOpen",
              },
              {
                scheme: "file",
                type: "general.audio",
                "maxFileSupported": 1
              },
              {
                scheme: "file",
                type: "general.video",
                "maxFileSupported": 1
              },
            ],
          },
        ],
      },
      {
        "name": "HDRAbility",
        "srcEntry": "./ets/hdrability/HDRAbility.ets",
        "description": "$string:HDRAbility_desc",
        "icon": "$media:icon",
        "label": "$string:HDRAbility_label",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background"
      }
    ],
    requestPermissions: [
      {
        name: "ohos.permission.INTERNET"
      },
      {
        name: "ohos.permission.FILE_ACCESS_PERSIST"
      },
      {
        name: "ohos.permission.GET_WIFI_INFO"
      },
      {
        name: "ohos.permission.GET_NETWORK_INFO"
      },
      {
        name: "ohos.permission.VIBRATE"
      },
      {
        name: "ohos.permission.KEEP_BACKGROUND_RUNNING",
        reason: "$string:reason_keep_background_running",
        usedScene: {
          abilities: [
            "EntryAbility"
          ],
          when: "always",
        },
      },
    ],
  },
}